##### VARIABLES

variable "image_id" {
  type = string
}

variable "availability_zone_names" {
  type    = list(string)
  default = ["us-west-1a"]
}

#### OUTPUT VALUES

output "instance_ip_addr" {
  value = aws_instance.server.private_ip
}


#### DATA SOURCES

data "aws_ami" "example" {
  most_recent = true

  owners = ["self"]
  tags = {
    Name   = "app-server"
    Tested = "true"
  }
}

